<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jaelyn.integrated.module.face.dao.UserFaceInfoDao">

    <select id="findUserFaceInfoList" resultType="com.jaelyn.integrated.module.face.model.entity.UserFaceInfo">
        SELECT name,create_time from user_face_info
    </select>

    <insert id="insertUserFaceInfo" parameterType="com.jaelyn.integrated.module.face.model.entity.UserFaceInfo">
            INSERT INTO user_face_info (group_id,face_id,name,face_feature) VALUES (#{groupId},#{faceId},#{name},#{faceFeature})
    </insert>

    <select id="getUserFaceInfoByGroupId" resultType="com.jaelyn.integrated.module.face.model.dto.FaceUserInfo"
            parameterType="java.lang.Integer">
        select face_id,name,face_feature from user_face_info where group_id = #{groupId}
    </select>
</mapper>
